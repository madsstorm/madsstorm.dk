{
  "hosting": {
    "public": "_site",
    "cleanUrls": true,
    "trailingSlash": false,
    "ignore": [ "firebase.json", "**/.*", "**/node_modules/**"],
    "headers": [{
      "source" : "**",
      "headers": [{
        "key": "X-Frame-Options",
        "value": "DENY"
      },
      {
        "key": "X-XSS-Protection",
        "value": "1; mode=block"
      },
      {
        "key": "Referrer-Policy",
        "value": "no-referrer"
      },
      {
        "key": "Content-Security-Policy",
        "value": "default-src https:; object-src 'none'; media-src 'none'"
      }]
    }],
    "redirects": [{
      "source": "http://madsstormdk.web.app/:path",
      "destination": "https://madsstorm.dk/:path",
      "type": 301
    },
    {
      "source": "https://madsstormdk.web.app/:path",
      "destination": "https://madsstorm.dk/:path",
      "type": 301
    },
    {
      "source": "http://madsstormdk.firebaseapp.com/:path",
      "destination": "https://madsstorm.dk/:path",
      "type": 301
    },
    {
      "source": "https://madsstormdk.firebaseapp.com/:path",
      "destination": "https://madsstorm.dk/:path",
      "type": 301
    }]
  }
}
